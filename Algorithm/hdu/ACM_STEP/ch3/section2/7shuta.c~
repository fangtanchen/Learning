#include<stdio.h>
#include<string>



int max(int x,int y){
  return x>y?x:y;
}

int main()
{
  int C;
  scanf("%d",&C);
  int n;
  int dp[100][100];
  int num[100];

  while(C--){
    scanf("%d",&n);
    for(int i=0;i<n;i++){
      for(int j=0;j<n;j++){
	scanf("%d",&num[i][j]);
      }
    }

  }

  return 0;
}
